{
  
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": ["reactjs-availability-calendar"]
        }
      ]
  }
}